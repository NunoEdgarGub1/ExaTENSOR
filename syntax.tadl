#Tensor Algebra Description Language (TADL) syntax:

basis(real|complex) basis0=[1:1000],basis1=[0:893]

range(basis0) occ=[1:100],virt=[101:1000]

index(occ) i0,i1,k0,k1

index(virt) a0,a1,c0,c1


read T2(a0,a1,i0,i1)

V(i0,i1,a0,a1)=method(integrals_eri): [i0<i1],[a0<a1],sparse(scheme0)

Z2(a0,a1,i0,i1)=V(i0,k0,a0,c0)*T2(c0,a1,k0,i1): [i0<i1],[a0<a1],sparse(scheme1)

write Z2(a0,a1,i0,i1),V

destroy V,T2,Z2(a0,a1,i0,i1)
